{
  "name": "wumvi/wm.php.http-utils",
  "require": {
    "ext-json": "*",
    "php": "^8.3",
    "firebase/php-jwt": "^v6.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "10.5.3",
    "squizlabs/php_codesniffer": "3.8.0",
    "overtrue/phplint": "9.0.6",
    "phpstan/phpstan": "1.10.50",
    "phan/phan": "5.4.2"
  },
  "autoload": {
    "psr-4": {
      "Wumvi\\HttpUtils\\": "src"
    }
  },
  "archive": {
    "exclude": ["phpunit", "bootstrap.php", "phpstan.neon", "phpunit.xml", "README.md", "ruleset.xml", ".phpstorm.meta.php", ".travis.yml", ".phplint.yml", ".gitignore"]
  },
  "scripts": {
    "phpstan": [
      "phpstan analyze -c phpstan.neon"
    ],
    "phpunit": [
      "XDEBUG_MODE=coverage php -d extension=uopz ./vendor/bin/phpunit"
    ],
    "phplint": [
      "phplint"
    ],
    "phpcs": [
      "phpcs src --parallel=10 --runtime-set php_version 80800 -p --standard=ruleset.xml"
    ],
    "test": [
      "@phpstan",
      "@phpunit",
      "@phplint",
      "@phpcs"
    ]
  },
  "license": "proprietary"
}
